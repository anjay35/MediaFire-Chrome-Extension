{
  "name" : "MediaFire Extension",
  "version" : "1.0.5",
  "description" : "Upload files from the web directly to MediaFire",
  "background" : { "scripts": ["background.js", "jquery.js"] },
  "permissions" : [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*"
   ],
  "minimum_chrome_version" : "6.0.0.0",
  "icons" : {
    "16" : "icon.png",
    "48" : "icon.png",
    "128" : "icon.png"
  },
  "manifest_version": 2,
"browser_action": {
    "default_icon": "icon.png",
    "default_popup": "info.html"
  }
}
